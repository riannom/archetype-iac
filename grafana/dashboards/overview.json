{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Job Duration P95",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(archetype_job_duration_seconds_bucket[5m])) by (le, action))",
          "legendFormat": "P95 {{action}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      },
      "datasource": { "type": "prometheus", "uid": "prometheus" }
    },
    {
      "id": 2,
      "title": "Active Jobs",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(archetype_jobs_active)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "datasource": { "type": "prometheus", "uid": "prometheus" }
    },
    {
      "id": 3,
      "title": "Agents Online",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "archetype_agents_online",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "datasource": { "type": "prometheus", "uid": "prometheus" }
    },
    {
      "id": 4,
      "title": "Enforcement Status",
      "type": "timeseries",
      "gridPos": { "h": 4, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "archetype_enforcement_pending",
          "legendFormat": "Pending",
          "refId": "A"
        },
        {
          "expr": "sum(rate(archetype_enforcement_total{result=\"success\"}[5m]))",
          "legendFormat": "Success/s",
          "refId": "B"
        },
        {
          "expr": "sum(rate(archetype_enforcement_total{result=\"failed\"}[5m]))",
          "legendFormat": "Failed/s",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "datasource": { "type": "prometheus", "uid": "prometheus" }
    },
    {
      "id": 5,
      "title": "Nodes by State",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum(archetype_nodes_total) by (state)",
          "legendFormat": "{{state}}",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "prometheus" }
    },
    {
      "id": 6,
      "title": "Log Explorer - Slow Operations",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "{service=\"api\"} | json | event=~\"nlm_phase|agent_operation\" | duration_ms > 10000",
          "refId": "A"
        }
      ],
      "datasource": { "type": "loki", "uid": "loki" }
    }
  ],
  "schemaVersion": 39,
  "tags": ["archetype", "overview"],
  "templating": { "list": [] },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Archetype: System Overview",
  "uid": "archetype-overview",
  "version": 1
}
