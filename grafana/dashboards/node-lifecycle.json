  "id": null,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "NLM Phase Duration P95",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(archetype_nlm_phase_duration_seconds_bucket{status=\"success\"}[5m])) by (le, phase))",
          "legendFormat": "{{phase}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 30 },
              { "color": "red", "value": 120 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" } },
      "datasource": { "type": "prometheus", "uid": "prometheus" }
    },
    {
      "id": 2,
      "title": "Phase Duration by Device Type",
      "type": "barchart",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(archetype_nlm_phase_duration_seconds_bucket{status=\"success\"}[1h])) by (le, device_type, phase))",
          "legendFormat": "{{device_type}} - {{phase}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      },
      "datasource": { "type": "prometheus", "uid": "prometheus" }
    },
    {
      "id": 3,
      "title": "Deploy Duration Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(archetype_nlm_phase_duration_seconds_bucket{phase=\"container_deploy\",status=\"success\"}[5m])) by (le, device_type))",
          "legendFormat": "P95 deploy - {{device_type}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(archetype_nlm_phase_duration_seconds_bucket{phase=\"container_deploy\",status=\"success\"}[5m])) by (le, device_type))",
          "legendFormat": "P50 deploy - {{device_type}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      },
      "datasource": { "type": "prometheus", "uid": "prometheus" }
    },
    {
      "id": 4,
      "title": "Boot Wait Duration by Device Type",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(archetype_nlm_phase_duration_seconds_bucket{phase=\"boot_wait\",status=\"success\"}[5m])) by (le, device_type))",
          "legendFormat": "P95 boot - {{device_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      },
      "datasource": { "type": "prometheus", "uid": "prometheus" }
    },
    {
      "id": 5,
      "title": "Agent Operation Latency (API Round-Trip)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(archetype_agent_operation_duration_seconds_bucket{status=\"success\"}[5m])) by (le, operation))",
          "legendFormat": "P95 {{operation}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
